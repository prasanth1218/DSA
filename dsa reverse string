class Solution(object):
    def reverseString(self, s):
      left, right = 0, len(s) - 1
        while left < right:
          s[left], s[right] = s[right], s[left]
           left += 1
           right -= 1
# this code was done in leetcode with the source of input
# input=['h','e','l','l','o']
#output=['o','l','l','e','h']
